{
  "name": "@building-block/xhr-track-progress",
  "version": "0.0.0",
  "description": "Track progress, bitrate (speed), remaining time from XHR upload & download events",
  "repository": "building-block/blocks",
  "license": "MIT",
  "authors": [
    "Jake Murzy",
    "Julia Qiu"
  ],
  "main": "dist/xhrTrackProgress.js",
  "umd:main": "dist/xhrTrackProgress.umd.js",
  "module": "dist/xhrTrackProgress.mjs",
  "types": "dist/xhrTrackProgress.d.ts",
  "source": "src/xhrTrackProgress.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc",
    "prebuild": "npm run typecheck",
    "build": "microbundle --target web --strict && cp dist/xhrTrackProgress.mjs dist/xhrTrackProgress.es.js",
    "dev": "ts-node src/xhrTrackProgress",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "microbundle": "^0.11.0",
    "ts-node": "^8.3.0",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "typescript": "^3.5.2"
  },
  "keywords": [
    "XHR",
    "XMLHttpRequest",
    "fetch",
    "upload",
    "download",
    "stats",
    "progress",
    "react",
    "bitrate",
    "byte",
    "speed",
    "metrics",
    "percentage",
    "percent",
    "time remaining",
    "elapsed time",
    "uploader",
    "abortable",
    "abortable fetch"
  ]
}
